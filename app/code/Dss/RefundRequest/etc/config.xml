<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <dss_refundrequest>
            <dss_refundrequest_general>
                <enable>0</enable>
            </dss_refundrequest_general>
            <dss_refundrequest_config>
                <popup_title>refund request</popup_title>
                <popup_description>We are sorry to know that you want a refund, Plase let us know what's wrong?</popup_description>
                <dropdown_title>Reason for refund</dropdown_title>
                <option_title>Product is opened</option_title>
                <detail_title>Faulty or other detail</detail_title>
            </dss_refundrequest_config>
        </dss_refundrequest>
    </default>
</config>
